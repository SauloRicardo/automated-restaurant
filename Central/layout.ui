<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="frm_main">
    <property name="height">400</property>
    <property name="width">600</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">False</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.LabelFrame" id="frm_mesas">
        <property name="height">590</property>
        <property name="text" translatable="yes">Mesas</property>
        <property name="width">400</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">False</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="tk.Listbox" id="lstbox_mesas">
            <property name="cursor">arrow</property>
            <property name="exportselection">true</property>
            <property name="font">TkDefaultFont</property>
            <property name="height">23</property>
            <property name="listvariable">string:var_mesas</property>
            <property name="width">55</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.LabelFrame" id="frm_buttons">
        <property name="height">590</property>
        <property name="text" translatable="yes">Controle</property>
        <property name="width">200</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">False</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="tk.Button" id="btn_usar_mesa">
            <property name="text" translatable="yes">Usar Mesa</property>
            <bind add="True" handler="on_click_btn_usar_mesa" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">0</property>
              <property name="padx">55</property>
              <property name="propagate">False</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="Label_1">
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="btn_realizar_pedido">
            <property name="text" translatable="yes">Realizar Pedido</property>
            <bind add="True" handler="on_click_btn_realizar_pedido" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">0</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="btn_consultar_comanda">
            <property name="text" translatable="yes">Consultar Comanda</property>
            <bind add="True" handler="on_click_btn_consultar_comanda" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="btn_fechar_comanda">
            <property name="text" translatable="yes">Fechar Comanda</property>
            <bind add="True" handler="on_click_btn_fechar_comanda" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">0</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
